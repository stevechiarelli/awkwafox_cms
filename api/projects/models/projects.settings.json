{
  "kind": "collectionType",
  "collectionName": "projects",
  "info": {
    "name": "Projects"
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string",
      "required": true
    },
    "date": {
      "type": "date",
      "required": true
    },
    "category": {
      "type": "enumeration",
      "enum": [
        "videography",
        "livestream",
        "webdesign"
      ],
      "required": true,
      "default": "videography",
      "enumName": "category"
    },
    "featured": {
      "type": "boolean",
      "required": true
    },
    "image": {
      "model": "file",
      "via": "related",
      "allowedTypes": [
        "images",
        "files",
        "videos"
      ],
      "plugin": "upload",
      "required": true,
      "pluginOptions": {}
    },
    "description": {
      "type": "text",
      "required": true
    },
    "url": {
      "type": "string",
      "required": true
    },
    "buttonText": {
      "type": "string",
      "required": true
    },
    "buttonURL": {
      "type": "string",
      "required": true
    }
  }
}
