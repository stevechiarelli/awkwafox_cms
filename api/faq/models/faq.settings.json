{
  "kind": "collectionType",
  "collectionName": "faqs",
  "info": {
    "name": "FAQ"
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "question": {
      "type": "text",
      "required": true
    },
    "answer": {
      "type": "text",
      "required": true
    },
    "category": {
      "type": "enumeration",
      "enum": [
        "videography",
        "livestream",
        "webdesign"
      ],
      "required": true,
      "enumName": "category"
    },
    "active": {
      "type": "boolean",
      "required": true
    }
  }
}
